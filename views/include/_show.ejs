<% for(var i = 0; i < users.length; i++) { %>
    <tr>
        <td><%= i + 1 %></td>
        <td><%= users[i].title %></td>
        <td><%= users[i].content %></td>
        <td><%= users[i].createdOn %></td>
        
        <td>
            <a href="/update-note?id=<%= users[i]._id%>" class="btn border-shadow update">
                <span class="text-gradient"><i class="fas fa-edit"></i></span>
            </a>
            <a class="btn border-shadow delete" data-id=<%= users[i]._id%> >
                <span class="text-gradient"><i class="fas fa-trash"></i></span>
            </a>
        </td>
    </tr>
<% } %>


